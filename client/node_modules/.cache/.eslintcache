engine/client/src/index.js",[],[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/App.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/pages/SavedBooks.js",["64","65","66","67","68","69","70"],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/pages/SearchBooks.js",["71"],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/components/Navbar.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/components/SignupForm.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/components/LoginForm.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/localStorage.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/auth.js",["72"],[],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // check if user's logged in\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return !!token && !this.isTokenExpired(token); // handwaiving here\n  }\n\n  // check if token is expired\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","/Users/Victoria/bootcamp/book-search-engine/client/src/utils/API.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/queries.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/mutations.js",[],[],{"ruleId":"73","severity":1,"message":"74","line":1,"column":17,"nodeType":"75","messageId":"76","endLine":1,"endColumn":25},{"ruleId":"73","severity":1,"message":"77","line":12,"column":8,"nodeType":"75","messageId":"76","endLine":12,"endColumn":12},{"ruleId":"73","severity":1,"message":"78","line":13,"column":10,"nodeType":"75","messageId":"76","endLine":13,"endColumn":22},{"ruleId":"73","severity":1,"message":"79","line":17,"column":11,"nodeType":"75","messageId":"76","endLine":17,"endColumn":18},{"ruleId":"73","severity":1,"message":"80","line":19,"column":9,"nodeType":"75","messageId":"76","endLine":19,"endColumn":19},{"ruleId":"81","severity":2,"message":"82","line":48,"column":8,"nodeType":"75","messageId":"83","endLine":48,"endColumn":22},{"ruleId":"81","severity":2,"message":"84","line":75,"column":77,"nodeType":"75","messageId":"83","endLine":75,"endColumn":93},{"ruleId":"73","severity":1,"message":"85","line":27,"column":22,"nodeType":"75","messageId":"76","endLine":27,"endColumn":27},{"ruleId":"86","severity":1,"message":"87","line":49,"column":1,"nodeType":"88","endLine":49,"endColumn":34},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'Auth' is defined but never used.","'removeBookId' is defined but never used.","'loading' is assigned a value but never used.","'removeBook' is assigned a value but never used.","no-undef","'userDataLength' is not defined.","undef","'handleDeleteBook' is not defined.","'error' is assigned a value but never used.","import/no-anonymous-default-export","Assign instance to a variable before exporting as module default","ExportDefaultDeclaration"]