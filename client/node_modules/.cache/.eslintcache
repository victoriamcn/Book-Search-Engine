},"/Users/Victoria/bootcamp/book-search-engine/client/src/index.js",[],[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/App.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/pages/SavedBooks.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/pages/SearchBooks.js",["64"],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/components/Navbar.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/components/SignupForm.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/components/LoginForm.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/localStorage.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/auth.js",["65"],[],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // check if user's logged in\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return !!token && !this.isTokenExpired(token); // handwaiving here\n  }\n\n  // check if token is expired\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","/Users/Victoria/bootcamp/book-search-engine/client/src/utils/API.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/queries.js",[],[],"/Users/Victoria/bootcamp/book-search-engine/client/src/utils/mutations.js",[],[],{"ruleId":"66","severity":1,"message":"67","line":27,"column":22,"nodeType":"68","messageId":"69","endLine":27,"endColumn":27},{"ruleId":"70","severity":1,"message":"71","line":49,"column":1,"nodeType":"72","endLine":49,"endColumn":34},"no-unused-vars","'error' is assigned a value but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign instance to a variable before exporting as module default","ExportDefaultDeclaration"]